  "homepage": "https://github.com/mq1/mq1.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "windicss": "^2.5.9"
  }
