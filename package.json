  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:windicss": "onchange '_site/**/*.html' -- windicss './_site/**/*.html' -t -o _site/tailwind.css -f tailwind.config.js",
    "build": "cross-env NODE_ENV=production run-s build:eleventy build:windicss",
    "build:eleventy": "eleventy",
    "build:windicss": "windicss './_site/**/*.html' -m -t -o _site/tailwind.min.css -f tailwind.config.js"
  },
